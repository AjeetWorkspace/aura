{
    "extends" : [ "plugin:raptor/recommended" ],
    "plugins" : [ "raptor" ],
    "rules" : {
        "raptor/no-inline-disable" : [
                1,
                {
                    "whitelist" : {
                        "raptor/no-set-timeout" : [ "**/aura-modules/src/test/modules/moduletest/async/async.js" ],
                        "dot-notation" : [ "**/aura-modules/src/test/modules/moduletest/exports/exports.js" ],
                        "no-console" : [ "**/src/test/modules/**/*.js" ],
                        "no-unused-vars" : [ "**/src/test/modules/**/*.js" ],
                        "raptor/no-compat" : [ "**/aura-modules/src/test/modules/moduletest/import-cmp/import-cmp.js" ],
                        "raptor/no-compat-execute" : [ "**/aura-modules/src/test/modules/moduletest/exports/exports.js" ],
                        "raptor/no-aura-libs" : [ "**/aura-modules/src/test/modules/moduletest/simple-cmp/simple-cmp.js" ],
                        "raptor/no-aura" : [ "**/aura-modules/src/test/modules/securemoduletest/bootstrap/bootstrap.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simple-cmp/simple-cmp.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simple-lib/simple-lib.js"],
                        "no-undef" : [ "**/aura-modules/src/test/modules/securemoduletest/bootstrap/bootstrap.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simple-cmp/simple-cmp.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simple-lib/simple-lib.js"],
                        "no-script-url" : [ "**/aura-modules/src/test/modules/moduletest/nav-item/nav-item.js"]
                    }
                } ]
    }
}
